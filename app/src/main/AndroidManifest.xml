<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.chengsheng.cala.htcm">

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/htcm_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity
            android:name=".LaunchActivity"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".views.activitys.RegisterActivity" />
        <activity
            android:name=".views.activitys.LoginActivity"
            android:launchMode="singleTask" />
        <activity android:name=".views.activitys.RetrievePWActivity" />
        <activity
            android:name=".views.activitys.HomePageActivity"
            android:launchMode="singleTask" />
        <activity android:name=".views.activitys.SettingActivity" />
        <activity android:name=".views.activitys.FAQActivity" />
        <activity android:name=".views.activitys.MyDevicesActivity" />
        <activity android:name=".views.activitys.AccountSettingActivity" />
        <activity android:name=".views.activitys.MessageActivity" />
        <activity android:name=".views.activitys.SecuritySettingsActivity" />
        <activity android:name=".views.activitys.CommonUseActivity" />
        <activity android:name=".views.activitys.FeedbackActivity" />
        <activity android:name=".views.activitys.AboutActivity" />
        <activity android:name=".views.activitys.GesturesUnlockActivity" />
        <activity android:name=".views.activitys.UserCardActivity" />
        <activity android:name=".views.activitys.FamilyManageActivity"></activity>
    </application>

</manifest>