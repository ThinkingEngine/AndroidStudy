<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.chengsheng.cala.htcm">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_SETTINGS" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/htcm_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity
            android:name=".LaunchActivity"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".views.activitys.RegisterActivity" />
        <activity
            android:name=".views.activitys.LoginActivity"
            android:launchMode="singleTask" />
        <activity android:name=".views.activitys.RetrievePWActivity" />
        <activity
            android:name=".views.activitys.HomePageActivity"
            android:launchMode="singleTask" />
        <activity android:name=".views.activitys.SettingActivity" />
        <activity android:name=".views.activitys.FAQActivity" />
        <activity android:name=".views.activitys.MyDevicesActivity" />
        <activity android:name=".views.activitys.AccountSettingActivity" />
        <activity android:name=".views.activitys.MessageActivity" />
        <activity android:name=".views.activitys.SecuritySettingsActivity" />
        <activity android:name=".views.activitys.CommonUseActivity" />
        <activity android:name=".views.activitys.FeedbackActivity" />
        <activity android:name=".views.activitys.AboutActivity" />
        <activity android:name=".views.activitys.GesturesUnlockActivity" />
        <activity android:name=".views.activitys.UserCardActivity" />
        <activity android:name=".views.activitys.FamilyManageActivity" />
        <activity android:name=".views.activitys.FamiliesDetailsActivity" />
        <activity android:name=".views.activitys.ExamAppointmentActivity" />
        <activity android:name=".views.activitys.AIAssistantActivity" />
        <activity android:name=".views.activitys.MyExamActivity" />
        <activity android:name=".views.activitys.ExamDetailsActivity" />
        <activity android:name=".views.activitys.ComboDetailActivity" />
        <activity android:name=".views.activitys.AffirmAppointmentActivity" />
        <activity android:name=".views.activitys.ModePaymentActivity" />
        <activity android:name=".views.activitys.PayResultActivity" />
        <activity android:name=".views.activitys.AddExamPersonActivity" />
        <activity android:name=".views.activitys.ExamReportActivity" />
        <activity android:name=".views.activitys.ExamReportDetailActivity" />
        <activity android:name=".views.activitys.ExamResultUnscrambleActivity" />
        <activity android:name=".views.activitys.BeforeExamActivity" />
        <activity android:name=".views.activitys.ExamOrderFormActivity" />
        <activity android:name=".views.activitys.OrderDetailActivity" />
        <activity android:name=".views.activitys.ExamReportCompareActivity" />

        <receiver android:name=".model.broadcasts.NeworkChangeReceiver">
            <intent-filter>
                <action android:name="android.net.conn.CONNECTIVITY_CHANGE" />
            </intent-filter>
        </receiver>

        <provider
            android:name="android.support.v4.content.FileProvider"
            android:authorities="com.example.cameraalbumtest.fileprovider"
            android:exported="false"
            android:grantUriPermissions="true">
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/paths" />
        </provider>

        <activity android:name=".views.activitys.ModeFamiliesInfoActivity" />
        <activity android:name=".views.activitys.BarCodeActivity" />
        <activity android:name=".views.activitys.IntelligentCheckActivity" />
        <activity android:name=".views.activitys.ServiceOrderActivity"></activity>
    </application>

</manifest>